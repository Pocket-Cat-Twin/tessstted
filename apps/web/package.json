{
  "name": "@yuyu/web",
  "version": "1.0.0",
  "description": "SvelteKit frontend for YuYu Lolita Shopping",
  "scripts": {
    "dev": "vite dev",
    "dev:windows": "$env:NODE_ENV='development'; vite dev --host localhost",
    "build": "vite build",
    "build:windows": "vite build",
    "preview": "vite preview",
    "start": "node start-server.js",
    "start:windows": "node start-server.js",
    "start:basic": "node build",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@sveltejs/adapter-node": "^5.0.1",
    "@sveltejs/kit": "^2.5.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.2",
    "@tailwindcss/typography": "^0.5.10",
    "@types/eslint": "^8.56.7",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.37.0",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "prettier-plugin-svelte": "^3.2.2",
    "svelte": "^4.2.12",
    "svelte-check": "^3.6.9",
    "tailwindcss": "^3.4.3",
    "tslib": "^2.6.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.8"
  },
  "dependencies": {
    "@lucia-auth/adapter-drizzle": "^1.0.7",
    "lucia": "^3.2.0",
    "zod": "^3.22.4",
    "@yuyu/shared": "workspace:*"
  },
  "type": "module"
}
